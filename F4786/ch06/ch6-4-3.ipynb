   "source": [
    "import gradio as gr\n",
    "\n",
    "def greet(name):\n",
    "    return \"你好: \" + name + \"!\"\n",
    "\n",
    "app = gr.Interface(fn=greet,\n",
    "                   inputs=\"text\",\n",
    "                   outputs=\"text\")\n",
    "app.launch(server_name=\"raspberrypi.local\")"
   ]
